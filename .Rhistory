library(d3po)
setwd("~/git/strategic_futures/viz")
rm(list = ls())
df.opp = read.csv("opps.csv")
df.threat = read.csv("threat.csv")
# Reading and cleaning up data
df.opp$Scenario.full = gsub(" $", "", df.opp$Scenario)
df.opp$Scenario.full = gsub("^GC$", "Good company", df.opp$Scenario.full)
df.opp$Scenario.full = gsub("^LW$", "Lone wolves", df.opp$Scenario.full)
df.opp$Scenario.full = gsub("^PS$", "The passing storm", df.opp$Scenario.full)
df.opp$Scenario.full = gsub("^SE$", "Sunrise in the east", df.opp$Scenario.full)
df.threat$Scenario.full = gsub(" $", "", df.threat$Scenario)
df.threat$Scenario.full = gsub("^GC$", "Good company", df.threat$Scenario.full)
df.threat$Scenario.full = gsub("^LW$", "Lone wolves", df.threat$Scenario.full)
df.threat$Scenario.full = gsub("^PS$", "The passing storm", df.threat$Scenario.full)
df.threat$Scenario.full = gsub("^SE$", "Sunrise in the east", df.threat$Scenario.full)
df.opp = df.opp %>%
mutate(Category = gsub("[ ]+$", "", Category))
df.threat = df.threat %>%
select(-Notes, -Initials) %>%
mutate(Category = gsub(" - [A-Za-z,/& ]+$", "", Category)) %>%
mutate(Category = gsub("[ ]+$", "", Category))
df.opp$Type = "Opportunity"
df.threat$Type = "Threat"
df.all = rbind(df.opp, df.threat)
##################################################
# Proportion of mission portfolios that contribute to cross-cutting themes (category) (Sankey diagram, threat&opps data)
summary.df = df.opp %>%
group_by(MP, Category) %>%
summarize(count = n()) %>%
filter(MP != "", Category != "") %>%
rename(source = MP, target = Category, value = count)
summary.df = summary.df %>% filter(!(target %in% c("Workforce", "Other", "Integration", "Prioritization & Efficiency", "")))
d3 = sankey(summary.df, edge.color = "input")
d3
